{
  "database": {
    "type": "mysql", // MySQL is the only supported database engine at this time
    "server": "127.0.0.1", // MySQL Server
    "username": "YourDBUser", // Database Username
    "password": "YourDBPassword", // Database Password
    "dbname": "YourDBName", // Database Name
    "prefix": "rnk_" // Table Prefix
  },
  "teamspeak": {
    "sq_ip": "ts.example.com", // Hostname or IP of TeamSpeak server to connect to
