    "password": "YourSQPassword", // ServerQuery Password
    "ts_port": 9987 // TeamSpeak port
  },
  "ranking": {
    "max_idle_time": 15, // Maximum time a client can be idle before ranking pauses (minutes)
    "ranks": [] // Array of server group IDs in order of level
  }
}
